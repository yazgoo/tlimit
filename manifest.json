{
  "manifest_version": 2,
  "version": "0.4",
  "name": "tlimit",
  "background": {
    "scripts": ["tlimit.js"]
  },
  "icons": {
    "48": "icons/tlimit-48.png",
    "32": "icons/tlimit-32.png"
  },
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_icon": "icons/tlimit-32.png",
    "default_title": "tlimit",
    "default_popup": "popup/tlimit.html"
  }
}
